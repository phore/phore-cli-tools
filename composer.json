{
    "name": "phore/cli-tools",
    "type": "library",
    "description": "Cli helper tools",
    "keywords": ["cli", "Secrets", "CloudIA"],
    "homepage": "https://infracamp.com/phore",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Leuffen",
            "email": "m@tth.es"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/log": "^1.1",
        "ext-readline": "*",
        "phore/core": "*"
    },
    "require-dev": {
        "phore/log": "*",
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "phore/log": "Enable advanced logging / out capabilities"
    },
    "autoload": {
        "psr-4": {
            "Phore\\CliTools\\Demo\\": "demo_app",
            "Phore\\CliTools\\": "src"
        }
    },
    "minimum-stability": "dev"
}
